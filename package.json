{
  "name": "hunt-codes",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@types/react-dom": "^16.9.6",
    "@types/typed.js": "^2.0.9",
    "classnames": "^2.3.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-feather": "^2.0.10",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^5.0.0",
    "sass": "^1.54.8",
    "typed.js": "^2.0.11",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "serve": "serve -s build/",
    "deploy": "aws s3 sync build/ s3://hunt.codes --acl public-read --delete",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:react/recommended"
    ]
  },
  "browserslist": [
    ">0.5%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/react": "^16.9.34",
    "@types/react-router-dom": "^5.1.4",
    "eslint-plugin-react": "^7.31.4"
  }
}
